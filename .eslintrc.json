{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "react-app",
    "standard"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
  },
  "globals":{
    "afterEach": true,
    "beforeEach": true,
    "describe": true,
    "expect": true,
    "fetch": true,
    "it": true,
    "shallowMount": true,
    "shallowMountView": true
  }
}
